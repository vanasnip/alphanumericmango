# Local environment file for act execution
# Copy this file to .env.local and fill in any required values

# GitHub token for API access (not required for basic act runs)
# GITHUB_TOKEN=your_github_token_here

# NPM token for publishing (not required for CI validation)
# NPM_TOKEN=your_npm_token_here

# Sonar token for code analysis (not required for local runs)
# SONAR_TOKEN=your_sonar_token_here

# Skip external service integrations in local runs
ACT_EXECUTION=true
SKIP_EXTERNAL_SERVICES=true